APP_NAME=Dragonfortune
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# If you want multiple PHP built-in server workers, run:
# `php artisan serve --no-reload`
# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

BACKTEST_RESULTS_DISK=local
BACKTEST_RESULTS_DIR=backtest-results

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# --- Dragonfortune / Integrations ---
# Coinglass
COINGLASS_API_URL=https://open-api-v4.coinglass.com/api
COINGLASS_API_KEY=
COINGLASS_TIMEOUT=15
COINGLASS_RETRIES=2

# Optional caching TTLs
# - COINGLASS_CACHE_TTL is minutes (macro overlay); others are seconds.
COINGLASS_CACHE_TTL=15
COINGLASS_ETF_CACHE_TTL=30
COINGLASS_VOLATILITY_CACHE_TTL=30
COINGLASS_FR_CACHE_TTL=10
COINGLASS_OI_CACHE_TTL=10
COINGLASS_LSR_CACHE_TTL=10
COINGLASS_LIQUIDATIONS_CACHE_TTL=10
COINGLASS_BASIS_CACHE_TTL=10
COINGLASS_SENTIMENT_CACHE_TTL=10

# CryptoQuant
CRYPTOQUANT_API_KEY=

# FRED (Macro overlay)
FRED_API_KEY=

# QuantConnect (Backtest management)
QC_BASE_URL=https://www.quantconnect.com/api/v2
QC_USER_ID=
QC_API_TOKEN=
QC_ORGANIZATION_ID=
QC_TIMEOUT=15

# Binance Spot API
BINANCE_SPOT_MODE=auto
BINANCE_SPOT_BASE_URL=https://api.binance.com
BINANCE_SPOT_DEFAULT_ACCOUNT=v1
BINANCE_SPOT_V1_LABEL=Spot v1 - Dragon Fortune
BINANCE_SPOT_V1_BASE_URL=https://api.binance.com
BINANCE_SPOT_V1_API_KEY=
BINANCE_SPOT_V1_API_SECRET=
BINANCE_SPOT_V2_LABEL=Spot v2 - Dragon Fortune
BINANCE_SPOT_V2_BASE_URL=https://api.binance.com
BINANCE_SPOT_V2_API_KEY=
BINANCE_SPOT_V2_API_SECRET=
# Legacy fallback (optional)
# BINANCE_SPOT_API_KEY=
# BINANCE_SPOT_API_SECRET=
BINANCE_SPOT_TIMEOUT=10
BINANCE_SPOT_RECV_WINDOW=5000
BINANCE_SPOT_VERIFY_SSL=true
BINANCE_SPOT_PROXY_BASE_URL=
BINANCE_SPOT_PROXY_TOKEN=
BINANCE_SPOT_PROXY_VERIFY_SSL=true
BINANCE_SPOT_STUB_DATA=false

# External API base URL (if frontend proxies to another service)
API_BASE_URL=http://localhost:5000
SPOT_MICROSTRUCTURE_API_URL=
SPOT_USE_COINGLASS=true
SPOT_STUB_DATA=true
